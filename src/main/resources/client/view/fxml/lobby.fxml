                </VBox>
                <Label fx:id="title" prefHeight="100.0" prefWidth="565.0" text="LOBBY" StackPane.alignment="TOP_LEFT" />
            </children>
        </StackPane>

      <StackPane maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="380.0" prefWidth="640.0" StackPane.alignment="BOTTOM_LEFT">
